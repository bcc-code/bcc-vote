{
  "host": "localhost",
  "port": 4040,
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "authentication": {
    "oauth": {
      "redirect": "VOTE_AUTH_SUCCESS_CLIENT_REDIRECT",
      "auth0": {
        "scope": ["openid", "profile", "email", "church", "country"],
        "key": "VOTE_AUTH0_CLIENT_KEY",
        "secret": "VOTE_AUTH0_CLIENT_SECRET",
        "subdomain": "VOTE_AUTH0_SUBDOMAIN"
      }
    },
    "entity": "user",
    "service": "users",
    "secret": "FEATHERS_JWT_HS256_SECRET",
    "authStrategies": [
      "jwt",
      "local",
      "auth0"
    ],
    "jwtOptions": {
      "header": {
        "typ": "access"
      },
      "audience": "https://login.bcc.no",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "usernameField": "email",
      "passwordField": "password"
    }
  },
  "nedb": "../data",
  "arangodDB": {
    "username": "VOTE_ARANGODB_USERNAME",
    "password": "VOTE_ARANGODB_PASSWORD",
    "url": "VOTE_ARANGODB_URL",
    "database": "VOTE_ARANGODB_DATABASE"
  },
  "members":{
    "url":"MEMBERS_URL",
    "apiKey":"MEMBERS_API_KEY"
  }
}
